    |        |              |       dbm-t
    |        |              +--ro tx-channel-power-max?
    |        |              |       dbm-t
    |        |              +--ro rx-channel-power-min?
    |        |              |       dbm-t
    |        |              +--ro rx-channel-power-max?
    |        |              |       dbm-t
